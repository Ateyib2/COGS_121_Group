<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/css/materialize.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/c3@0.6.0/c3.min.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp" crossorigin="anonymous">
    <link href="/css/home.css" rel="stylesheet">
    <title>Profile</title>
  </head>
  <body>
    <div class="navbar-fixed">
			<nav class="transparent">
				<div class="nav-wrapper transparent">
					<a href="#" class="brand-logo center grey-text">PetsApp</a>
					<ul id="nav-mobile" class="left">
        		<li><a href="/home">
  <img class="responsive-img icons header-icons" src="/imgs/home.png">
</a></li>
      		</ul>
				</div>
			</nav>
		</div>
    <div class="container">
      <h3>Profile</h3>
      <div class="row">
        <div class="col s12">
          <div class="row">
            <table class="centered responsive">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Age</th>
                  <th>Gender</th>
                  <th>Height</th>
                  <th>Weight</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td id="user_name">-</td>
                  <td id="user_age">-</td>
                  <td id="user_gender">-</td>
                  <td id="user_height">-</td>
                  <td id="user_weight">-</td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="white" id="chart1"></div>
    </div>
    <footer class="page-footer transparent">
      <div class="container">
        <div class="row">
          <div class="col s3 center-align">
            <a href="/goals">
              <!--
              <img class="responsive-img icons footer-icons" src="/imgs/goals-icon.png">
              -->
              <i class="fas fa-list fa-2x black-text"></i>
            </a>
          </div>
          <div class="col s3 center-align">
            <a href="/feed">
              <!--
              <img class="responsive-img icons footer-icons" src="/imgs/feed-icon.png">
              -->
              <i class="fas fa-utensils fa-2x black-text"></i>
            </a>
          </div>
          <div class="col s3 center-align">
            <a href="/house" class="center-align">
              <!--
              <img class="responsive-img center-align footer-icons" id="store-icon" src="/imgs/store-icon.png">
              -->
              <i class="fas fa-home fa-2x black-text"></i>
            </a>
          </div>
          <div class="col s3 center-align">
            <a href="/profile" class="center-align">
              <img class="responsive-img center-align footer-icons" src="/imgs/cat-profile.png">
            </a>
          </div>
        </div>
      </div>
    </footer>

    <!-- Scripts -->
    <script src="http://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/js/materialize.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/d3@5.4.0/dist/d3.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/c3@0.6.0/c3.min.js"></script>
    <script src="js/profile.js" charset="utf-8"></script>
  </body>
</html>
